    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.2",
    "inquirer": "^9.1.4",
    "lint-staged": "^13.0.3",
    "lorem-ipsum": "^2.0.8",
    "metro-react-native-babel-preset": "^0.66.2",
