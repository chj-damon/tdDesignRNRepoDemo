{
  "name": "rn-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android:dev": "cross-env ENVFILE=.env.dev react-native run-android",
    "android:staging": "cd ./android && cross-env ENVFILE=.env.staging ./gradlew app:assembleRelease",
    "android:prod": "cd ./android && cross-env ENVFILE=.env.prod ./gradlew app:assembleRelease",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "prettier": "prettier --write --cache --cache-strategy metadata \"**/*.{tsx,ts}\"",
    "clean": "react-native-clean-project",
    "postinstall": "npx husky install && npx patch-package",
    "commit": "git-cz",
    "tsc": "tsc -p tsconfig.json",
    "pod": "cd ios && pod install && cd .."
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md}": [
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@byron-react-native/refresh-control": "^1.2.5",
    "@ebay/nice-modal-react": "^1.2.8",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-native-community/netinfo": "^9.3.6",
    "@react-native-picker/picker": "^2.4.8",
    "@react-navigation/bottom-tabs": "^6.4.1",
    "@react-navigation/native": "^6.0.14",
    "@react-navigation/native-stack": "^6.9.2",
    "@shopify/restyle": "2.0.0",
    "@td-design/react-native": "^3.11.15",
    "@td-design/react-native-echarts": "^5.4.0",
    "@td-design/react-native-image-picker": "^1.6.2",
    "@td-design/react-native-picker": "^1.9.3",
    "@td-design/rn-hooks": "^2.4.0",
    "dayjs": "^1.11.6",
    "immer": "^9.0.16",
    "jotai": "^1.10.0",
    "lodash-es": "^4.17.21",
    "react": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-native": "0.66.5",
    "react-native-background-timer": "^2.4.1",
    "react-native-bootsplash": "^4.3.3",
    "react-native-config": "^1.4.11",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.8.0",
